@media screen and (min-width: 1620px) {
  .row.shift-up {
    > * {
      margin-top: -573px;
    }
  }
}

@media screen and (max-width: 1620px) {
  .card.feed-panel.large-card {
    height: 824px;
  }
}

.user-stats-card {
  .card-title {
    padding: 0 0 15px;
  }
}

.blurCalendar {
  height: 475px;
}

.demo-chart {
  height: 600px;
}

.loading {
  width: 160px;
  height: 56px;
  position: fixed;
  top: 50%;
  left: 45%;
  line-height: 56px;
  color: #fff;
  padding-left: 60px;
  font-size: 15px;
  background: #000 /*url(../../../assets/img/loader.gif) */no-repeat 10px 50%;
  opacity: 0.7;
  z-index: 9999;
  -moz-border-radius: 20px;
  -webkit-border-radius: 20px;
  border-radius: 20px;
  filter: progid:DXImageTransform.Microsoft.Alpha(opacity=70);
}

.warning {
  width: 200px;
  height: 56px;
  position: fixed;
  top: 50%;
  left: 45%;
  line-height: 56px;
  color: #fff;
  text-align: center;
  font-size: 15px;
  background: #ff0000;
  filter: alpha(opacity=100);
  -moz-opacity: 1.0;
  opacity: 1.0;
  z-index: 9999;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
}
